<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="classes" thread-count="5" name="All Test Suite">
    <listeners>
        <listener class-name="RESOURCES.Listeners"></listener>
    </listeners>
    <test  thread-count="5" parallel="classes" verbose="2" preserve-order="true" name="ChromeDemo">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="test"/>
            </run>
        </groups>
        <classes>
          <class name="TESTS.CheckoutTests" />
       </classes>
    </test>
</suite>